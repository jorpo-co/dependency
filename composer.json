{
    "name": "jorpo/dependency",
    "description": "Dependency Injection abstraction using Auryn",
    "type": "library",
    "license": "GNU GPLv3",
    "authors": [
        {
            "name": "John Porter",
            "email": "john@designermonkey.co.uk"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "require": {
        "php": "^7.4",
        "rdlowrey/auryn": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Jorpo\\Dependency\\": [
                "src"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "phpmetrics/phpmetrics": "^2.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Jorpo\\Dependency\\": [
                "tests"
            ]
        }
    }
}
